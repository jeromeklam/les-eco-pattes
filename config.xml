<?xml version='1.0' encoding='utf-8'?>
<widget id="fr.freeasso.lesecopattes" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>LesEcoPattes</name>
    <description>
        Les Eco-Pattes
    </description>
    <author email="jeromeklam@free.fr" href="https://github.com/jeromeklam">
        Jérôme KLAM
    </author>
    <content src="index.html" />
    <hook type="before_prepare" src="scripts/cordova.js" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
